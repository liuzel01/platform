        "phpdocumentor/type-resolver": "~1.7.1",
        "phpstan/extension-installer": "^1.3.0",
        "phpstan/phpstan": "1.10.15",
        "phpstan/phpstan-deprecation-rules": "1.1.3",
        "phpstan/phpstan-doctrine": "1.3.40",
        "phpstan/phpstan-phpunit": "1.3.11",
