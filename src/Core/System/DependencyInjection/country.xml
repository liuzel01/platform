        <service id="Shuwei\Core\System\Country\SalesChannel\SalesChannelCountryDefinition">
            <tag name="shuwei.sales_channel.entity.definition"/>
        </service>

        <service id="Shuwei\Core\System\Country\Aggregate\CountryState\CountryStateDefinition">
